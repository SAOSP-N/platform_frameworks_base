<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Show voice assist affordance on Keyguard -->
    <bool name="config_keyguardShowVoiceAssistAffordance">false</bool>

    <integer name="config_qs_num_rows_landscape_default">2</integer>
    <integer name="config_qs_num_columns_landscape_default">3</integer>

    <!-- Allow Opa (Pixel navbar) layout -->
    <bool name="config_allowOpaLayout">true</bool>

    <!-- Clear-All recents FAB animation -->
    <integer name="recents_animate_fab_enter_duration">225</integer>
    <integer name="recents_animate_fab_enter_delay">275</integer>
    <integer name="recents_animate_fab_exit_duration">125</integer>

    <!-- Nav bar button swapped ordering/layout -->
    <string name="config_navBarLayoutSwapped" translatable="false">menu_ime,recent;home;back,space</string>

</resources>
